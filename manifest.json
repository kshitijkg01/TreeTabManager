{
  "manifest_version": 2,

  "name": "Tree tab Manager",
  "description": "UIUC SIGCHI Tree tab manager",
  "version": "1.0",

  "browser_action": {
    "default_icon": "calculator-128.png",
    "default_popup": "hello.html",
    "default_title": "New Tag"
  },
"background": {
  "scripts": ["js/event.js"],
  "persistent":false
},
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}