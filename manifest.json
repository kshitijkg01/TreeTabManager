{

  "name": "Tree tab Manager",
  "description": "UIUC SIGCHI Tree tab manager",
  "version": "0.9",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["js/event.js"],
    "persistent":false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Treetab",
    "default_popup": "graph_view.html"
  },  
  "web_accessible_resources": [
    "js/d3.min.js",
    "js/jquery-2.1.3.min.js"
  ],
  "manifest_version": 2

}