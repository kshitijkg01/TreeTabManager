{
  "manifest_version": 2,

  "name": "Tree tab Manager",
  "description": "UIUC SIGCHI Tree tab manager",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "hello.html"
  },
  "browser_action": {
    "default_icon": "calculator-128.png",
    "default_popup": "hello.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}