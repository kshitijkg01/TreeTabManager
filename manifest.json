{
  "name": "Tree tab Manager",
  "description": "UIUC SIGCHI Tree tab manager",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["js/event.js"],
    "persistent":false
  },
  "content_scripts":[
    {
      "matches": ["<all_urls"],
      "js": ["js/jquery-2.1.3.min.js"]
    }
  ]
  ,
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Treetab",
    "default_popup": "graph_view.html"
  },  
  "manifest_version": 2
}